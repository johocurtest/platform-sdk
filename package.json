{
  "name": "@withthegrid/platform-sdk",
  "version": "11.0.1",
  "description": "SDK for withthegrid platform",
  "license": "MIT",
  "files": [
    "/esm",
    "/cjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/withthegrid/platform-sdk.git"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "start": "npm run watch-ts",
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "rm -rf cjs/ && tsc -p tsconfig.cjs.json",
    "build:esm": "rm -rf esm/ && tsc",
    "postpublish": "npm publish --ignore-scripts --tag latest --@withthegrid:registry='https://npm.pkg.github.com'",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "watch-ts": "rm -rf esm/ && tsc -w",
    "lint": "npm run build:cjs && eslint \"src/*.{js,ts}\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll"
  },
  "author": "Rob Everhardt <rob@withthegrid.com>",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/node": "^13.9.8",
    "@types/qs": "^6.9.1",
    "axios": "^0.19.2",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
